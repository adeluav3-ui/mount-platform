{
    "rewrites": [
        // OneSignal service worker routes - MUST COME FIRST
        {
            "source": "/OneSignalSDKWorker.js",
            "destination": "https://cdn.onesignal.com/sdks/OneSignalSDKWorker.js"
        },
        {
            "source": "/OneSignalSDKWorker.js/:path*",
            "destination": "https://cdn.onesignal.com/sdks/OneSignalSDKWorker.js"
        },
        {
            "source": "/OneSignalSDKUpdaterWorker.js",
            "destination": "https://cdn.onesignal.com/sdks/OneSignalSDKUpdaterWorker.js"
        },
        {
            "source": "/OneSignalSDKUpdaterWorker.js/:path*",
            "destination": "https://cdn.onesignal.com/sdks/OneSignalSDKUpdaterWorker.js"
        },
        {
            "source": "/OneSignalSDK.sw.js",
            "destination": "https://cdn.onesignal.com/sdks/web/v16/OneSignalSDK.sw.js"
        },
        {
            "source": "/OneSignalSDK.sw.js/:path*",
            "destination": "https://cdn.onesignal.com/sdks/web/v16/OneSignalSDK.sw.js"
        },
        // Your existing SPA route - KEEP THIS LAST
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                }
            ]
        }
    ]
}